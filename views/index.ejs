<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Excel Read Write</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  </head>
  <body>
    <div class="container-fluid">
      <div class="row">
        <form method="POST" action="/readExcelFile" enctype="multipart/form-data">
          <div class="col-md-6 mb-3">
            <label for="excel_file" class="form-label">Default file input example</label>
            <input class="form-control" type="file" id="excel_file" name="excel_file">
          </div>
          <div class="col-md-6 mb-3">
            <button class="btn btn-primary" type="submit">Submit</button>
          </div>
        </form>
        
      </div>
      <div class="row">
        <div class="col-md-6">
          <a href="/writeExcelFile" class="btn btn-success">Write Data onto Excel File</a>
          <a href="/appendExcelFile" class="btn btn-secondary">Append Data onto existing Excel File</a>
          <!-- <a href="/downloadRandomFile" class="btn btn-warning">Download a Random File</a> -->
        </div>

      </div>

    </div>
    

    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
  </body>
</html>